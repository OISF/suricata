[package]
name = "suricata"
version = "@PACKAGE_VERSION@"

[lib]
crate-type = ["staticlib"]

[profile.release]
debug = true

[features]
lua = []
experimental = ["ntp-parser", "ipsec-parser"]
strict = []
debug = []

[dependencies]
nom = "~3.2.1"
libc = "^0.2.36"
crc = "~1.7.0"
der-parser = "0.5.2"
kerberos-parser = "0.1.0"

ntp-parser = { version = "^0", optional = true }
ipsec-parser = { version = "0.3", optional = true }
