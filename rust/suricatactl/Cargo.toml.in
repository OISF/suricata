[package]
name = "suricatactl"
version = "@PACKAGE_VERSION@"
edition = "2021"
license = "GPL-2.0-only"
description = "Suricata control tools"

[[bin]]
name = "suricatactl"

[dependencies]
regex = "~1.5.6"
tracing = "0.1"
tracing-subscriber = "0.3"
once_cell = { version = "1.21.3" }
clap = { version = "4.5.39", default-features = false, features = ["std", "derive", "help", "usage"] }
